    "def sum_of_digits(n):\n",
    "    if n >= 0 and n <= 9:\n",
    "        return n\n",
    "    else:\n",
    "        return n%10 + sum_of_digits(n//10)\n",
    "number = int(input())\n",
    "print(sum_of_digits(number))"
