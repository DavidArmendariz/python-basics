    "else:\n",
    "    a1 = 0\n",
    "    a2 = 1\n",
    "    i = 3\n",
    "    while i <= n:\n",
    "        fn = a1 + a2\n",
    "        a1 = a2\n",
    "        a2 = fn\n",
    "        i += 1\n",
    "    print(fn)"
