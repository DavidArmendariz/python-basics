    "def isprime(x):\n",
    "    if x == 1 or x == 0:\n",
    "        return False\n",
    "    for i in range(2, x):\n",
    "        if x % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "number = int(input())\n",
    "if isprime(number):\n",
    "    print('YES')\n",
    "else:\n",
    "    print('NO')"
   ]
